{
  "name": "wio",
  "version": "0.2.3",
  "description": "An Embedded Development Environment",
  "main": "index.js",
  "scripts": {
    "preinstall": "node npm-go/preinstall.js",
    "postinstall": "node npm-go/postinstall.js",
    "preuninstall": "node npm-go/preuninstall.js"
  },
  "bin": "./bin/wio",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waterloop/wio.git"
  },
  "keywords": [
    "wio",
    "c",
    "c++",
    "avr",
    "arduino",
    "cosa"
  ],
  "author": "Deep Dhillon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/waterloop/wio/issues"
  },
  "homepage": "https://github.com/waterloop/wio#readme",
  "goBinary": {
    "name": "wio",
    "path": "./bin",
    "url": "https://github.com/waterloop/wio/releases/download/v{{version}}/wio_{{version}}_{{platform}}_{{arch}}.{{format}}"
  },
  "dependencies": {
    "decompress": "^4.2.0",
    "mkdirp": "^0.5.1",
    "request": "^2.87.0",
    "tar": "^2.2.1",
    "unzip": "^0.1.11"
  }
}
